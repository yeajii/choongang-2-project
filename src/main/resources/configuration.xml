        <typeAlias alias="DistributedHomeworks"     type="com.oracle.projectGo.dto.DistributedHomeworks"/>
        <typeAlias alias="gameContents"             type="com.oracle.projectGo.dto.GameContents"/>
        <typeAlias alias="Users"                    type="com.oracle.projectGo.dto.Users"/>
        <typeAlias alias="Board"                    type="com.oracle.projectGo.dto.Board"/>
        <typeAlias alias="payments"                 type="com.oracle.projectGo.dto.Payments"/>
        <typeAlias alias="LearningGroup"            type="com.oracle.projectGo.dto.LearningGroup"/>
        <typeAlias alias="LearningGroupMember"      type="com.oracle.projectGo.dto.LearningGroupMember"/>
        <typeAlias alias="EducationalResources"     type="com.oracle.projectGo.dto.EducationalResources"/>
    </typeAliases>
</configuration>